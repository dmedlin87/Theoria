{
  "name": "theo-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "THEO_AUTH_ALLOW_ANONYMOUS=1 jest",
    "test:vitest": "vitest run",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:types": "tsc --noEmit",
    "test:e2e": "playwright test --config=playwright.config.default.ts --reporter=line --reporter=html=playwright-report/default",
    "test:e2e:smoke": "playwright test --grep @smoke --config=playwright.config.smoke.ts --reporter=line --reporter=html=playwright-report/smoke",
    "test:e2e:full": "playwright test --grep @full --config=playwright.config.full.ts --reporter=line --reporter=html=playwright-report/full",
    "test:e2e:visual": "percy exec -- playwright test --grep @visual --reporter=line,html",
    "test:a11y": "playwright test --grep @a11y --reporter=line",
    "test:lighthouse:smoke": "node scripts/quality/run-lighthouse-smoke.mjs",
    "quality:gates": "node scripts/quality/check-quality-gates.mjs",
    "quality:baseline": "node scripts/quality/update-baseline.mjs",
    "quality:dashboard": "node scripts/quality/generate-dashboard.mjs"
  },
  "dependencies": {
    "@graphiql/toolkit": "^0.11.3",
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@tanstack/react-virtual": "^3.13.12",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "focus-trap-react": "^11.0.4",
    "graphiql": "^5.2.1",
    "lucide-react": "^0.545.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-dropzone": "^14.3.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "0.15.1",
    "@next/bundle-analyzer": "15.5.4",
    "@percy/cli": "^1.31.4",
    "@percy/playwright": "^1.0.9",
    "@playwright/test": "1.56.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "4.0.3",
    "msw": "^2.6.8",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "openapi-typescript": "^7.10.1",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3",
    "undici": "7.16.0",
    "vitest": "4.0.3"
  }
}