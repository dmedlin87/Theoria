{
  "name": "theo-web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "THEO_AUTH_ALLOW_ANONYMOUS=1 jest",
    "test:vitest": "vitest run --coverage",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:types": "tsc --noEmit",
    "test:e2e": "playwright test --reporter=line,html",
    "test:e2e:smoke": "playwright test --grep @smoke --reporter=line,html",
    "test:e2e:full": "playwright test --reporter=line,html"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.5.4",
    "@playwright/test": "1.56.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "29.5.12",
    "@types/node": "20.19.19",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitest/coverage-v8": "2.1.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "30.2.0",
    "openapi-typescript": "^7.9.1",
    "ts-jest": "29.4.5",
    "typescript": "5.7.2",
    "undici": "7.16.0",
    "vitest": "2.1.5"
  }
}
